config_version=5

[application]

config/name="Bcc"
run/main_scene="res://Scenes/MainMenu/MainMenu.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://Assets/UI/Icon/0.png"

[autoload]

Player="*res://Global/Player.gd"
Game="*res://Global/Game.gd"
TransitionScene="*res://Scenes/Reusables/Transition/TransitionScene.tscn"
SaveManager="*res://Global/SaveManager.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Aurora": "res://Dialogic/Characters/Aurora.dch",
"Dormogh": "res://Dialogic/Characters/Dormogh.dch",
"Dress": "res://Dialogic/Assets/Dress.dch",
"Gem": "res://Dialogic/Assets/Gem.dch",
"MagicCat": "res://Dialogic/Characters/MagicCat.dch",
"Margareta": "res://Dialogic/Characters/Margareta.dch",
"SellerCat": "res://Dialogic/Characters/SellerCat.dch"
}
directories/dtl_directory={
"0": "res://Dialogic/Timelines/MagicCat/0.dtl",
"1": "res://Dialogic/Timelines/MagicCat/1.dtl",
"2": "res://Dialogic/Timelines/MagicCat/2.dtl",
"3": "res://Dialogic/Timelines/MagicCat/3.dtl",
"4": "res://Dialogic/Timelines/MagicCat/4.dtl"
}
variables={}
layout/style_list=["res://Dialogic/BubbleStyle.tres", "res://Dialogic/VisualStyle.tres"]
